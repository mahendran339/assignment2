<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginCheck</servlet-name>
  	<display-name>LoginCheck</display-name>
  	<description></description>
  	<servlet-class>dbAccess.LoginCheck</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginCheck1</servlet-name>
  	<display-name>LoginCheck1</display-name>
  	<description></description>
  	<servlet-class>dbAccess.LoginCheck1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Feecalc</servlet-name>
  	<display-name>Feecalc</display-name>
  	<description></description>
  	<servlet-class>dbAccess.Feecalc</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveDetails</servlet-name>
  	<display-name>SaveDetails</display-name>
  	<description></description>
  	<servlet-class>dbAccess.SaveDetails</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginCheck</servlet-name>
  	<url-pattern>/LoginCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginCheck1</servlet-name>
  	<url-pattern>/LoginCheck1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Feecalc</servlet-name>
  	<url-pattern>/Feecalc</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveDetails</servlet-name>
  	<url-pattern>/SaveDetails</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
</welcome-file-list>
</web-app>
